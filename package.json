{
  "name": "tsed-api-boilerplate",
  "version": "0.0.3",
  "description": "REST API server using Ts.ED",
  "author": "Doga Genc <dogagenc@gmail.com>",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "bump": "npm version ${LEVEL:-patch} --force -m 'Version %s'"
  },
  "_moduleAliases": {
    "@src": "./dist"
  },
  "dependencies": {
    "@tsed/ajv": "5.60.0",
    "@tsed/common": "5.60.0",
    "@tsed/core": "5.60.0",
    "@tsed/di": "5.60.0",
    "@tsed/exceptions": "5.60.0",
    "@tsed/mongoose": "5.60.0",
    "@tsed/platform-express": "5.60.0",
    "@tsed/swagger": "5.60.0",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.2.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "express": "4.17.3",
    "method-override": "3.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "5.9.21"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "1.16.12",
    "@tsed/cli-plugin-mongoose": "1.16.12",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "5.7.29",
    "@types/node": "14.0.14",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "concurrently": "5.2.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.9.6"
  }
}
